<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
        "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
        "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib>
    <namespace>http://alertmonitor.io/facelets</namespace>
    <tag>
        <tag-name>tag</tag-name>
        <source>taglib/tag.xhtml</source>
    </tag>
    <tag>
        <tag-name>target</tag-name>
        <source>taglib/target.xhtml</source>
    </tag>
    <tag>
        <tag-name>singletag</tag-name>
        <component>
            <component-type>SingleTag</component-type>
        </component>
    </tag>
    <tag>
        <tag-name>tags</tag-name>
        <component>
            <component-type>MultiTag</component-type>
            <renderer-type>alertmonitor.MultiTagRenderer</renderer-type>
        </component>
    </tag>
</facelet-taglib>